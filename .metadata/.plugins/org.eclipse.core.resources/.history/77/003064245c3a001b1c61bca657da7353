package arreglos;

public class Ejercicio4 {
     
	
	
	
	public static boolean comprobarCreciente(int a[] , int tamano) {
		boolean bandera;
		
		bandera = false;
		
		for (int i = 0; i < tamano; i++) {
			
			 for (int j = i+1; j < a.length; j++) {
			
				  if( !(a[i]<a[j]) ) {
					  bandera = true;
					  break;
				  }
			}
			 
			if(bandera) {
				break;
			}
		}
		
		return bandera;
		
	}
}
